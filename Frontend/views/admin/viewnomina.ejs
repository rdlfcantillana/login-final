<div>
  <h1> Lista De Nomina</h1>
    <table>
        <tr>
            <th>Nombre</th>
            <th>Cargo</th>
            <th>Monto</th>
            <th>Fecha de Pago</th>
        </tr>
        <% nominas.forEach(function(nomina) { %>
            <tr>
                <td><%= nomina.nom_persona %></td>
                <td><%= nomina.de_cargo %></td>
                <td><%= nomina.monto %></td>
                <td><%= nomina.fecha_pago %></td>
            </tr>
        <% }); %>
